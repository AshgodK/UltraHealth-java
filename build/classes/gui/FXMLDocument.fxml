         </children>
      </VBox>
      <Button fx:id="btnInsert" layoutX="24.0" layoutY="293.0" mnemonicParsing="false" onAction="#savePost" text="Insert" />
      <Button layoutX="204.0" layoutY="293.0" mnemonicParsing="false" text="Delete" />
